unter("CryptText","CryptLabel","Результат")}function fileActionAES(e=0,t,n=""){const o=new FileReader;0===e?(o.onload=function(e){const o=CryptoJS.AES.encrypt(e.target.result,n),a=document.createElement("a");a.setAttribute("href","data:application/octet-stream,"+o),a.setAttribute("download",t.name+".encrypted"),a.click()},o.readAsDataURL(t)):1===e&&(o.onload=function(e){const o=CryptoJS.AES.decrypt(e.target.result,n).toString(CryptoJS.enc.Utf8),a=document.createElement("a");a.setAttribute("href",o),a.setAttribute("download",t.name.replace(".encrypted","")),a.click()},o.readAsText(t))}function fileActionBase64(e=0,t){const n=new FileReader;console.log(e),0===e?(n.onload=function(e){const n=utf8_to_b64(e.target.result),o=document.createElement("a");o.setAttribute("href","data:application/octet-stream,"+n),o.setAttribute("download",t.name+".encrypted"),o.click()},n.readAsDataURL(t)):1===e&&(n.onload=function(e){const n=b64_to_utf8(e.target.result).toString(),o=document.createElement("a");o.setAttribute("href",n),o.setAttribute("download",t.name.replace(".encrypted","")),o.click()},n.readAsText(t))}function fileActionSHA(e=0,t){const n=new FileReader;0===e&&(n.onload=function(e){let t="";t+="<p>SHA 224: "+CryptoJS.SHA224(e.target.result)+"<p>",t+="<p>SHA 256: "+CryptoJS.SHA256(e.target.result)+"<p>",t+="<p>SHA 384: "+CryptoJS.SHA384(e.target.result)+"<p>",t+="<p>SHA 512: "+CryptoJS.SHA512(e.target.result)+"<p>",t+="<p>SHA 3: "+CryptoJS.SHA3(e.target.result),document.getElementById("hash_answers").innerHTML=t},n.readAsDataURL(t))}function utf8_to_b64(e){return window.btoa(unescape(encodeURIComponent(e)))}function b64_to_utf8(e){try{return decodeURIComponent(escape(window.atob(e)))}catch(e){return"Error"}}function closeMenu(){document.querySelector(".mdl-layout__obfuscator").click()}function setPageMarkers(e){const t=document.querySelectorAll(".menuItemSelected");t.length&&t[0].classList.remove("menuItemSelected");let n=document.getElementById("menu_"+e);n&&n.classList.ad