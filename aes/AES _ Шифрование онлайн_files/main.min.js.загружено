)+" Системный язык: "+e.getSystemLanguage()}Date.prototype.format=function(e="yyyy-mm-dd"){const t={yyyy:this.getFullYear(),mm:("0"+(this.getMonth()+1)).slice(-2),dd:("0"+this.getDate()).slice(-2),hh:("0"+this.getHours()).slice(-2),MM:("0"+this.getMinutes()).slice(-2),ss:("0"+this.getSeconds()).slice(-2)};let n=e;for(const e in t)n=n.replace(e,t[e]);return n};